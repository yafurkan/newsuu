{
  "dart.flutterHotReloadOnSave": "never",
  "dart.buildRunnerAdditionalArgs": ["--delete-conflicting-outputs"],
  "dart.previewFlutterUiGuides": false,
  "dart.previewFlutterUiGuidesCustomTracking": false,
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/build/**": true,
    "**/.dart_tool/**": true,
    "**/android/.gradle/**": true,
    "**/android/app/build/**": true,
    "**/ios/build/**": true,
    "**/ios/.symlinks/**": true
  },
  "search.exclude": {
    "**/build": true,
    "**/.dart_tool": true,
    "**/.git": true,
    "**/android/.gradle": true,
    "**/android/app/build": true,
    "**/ios/build": true
  },
  "files.exclude": {
    "**/.dart_tool": true,
    "**/build": true
  }
}